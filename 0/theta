FoamFile
{
    format      ascii;
    class       volSymmTensorField;
    object      theta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (0 0 0 0 0 0);

boundaryField
{

    movingLid
    {
        type            zeroGradient;         
    }
    fixedWalls
    {
        type            zeroGradient;         
    }
    frontAndBack
    {
        type            empty;
    }

}

